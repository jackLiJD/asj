<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!-- 填充的颜色(白色) -->
    <stroke
        android:width="1dp"
        android:color="@color/colorPrimaryNew"
        android:dashGap="3dp"/>
    <!-- 设置按钮的四个角为弧形 -->
    <!-- android:radius 弧形的半径 -->
    <corners android:radius="80dp" />
</shape>