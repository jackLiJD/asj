<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!-- 填充的颜色(白色) -->
    <stroke
        android:width="1dp"
        android:color="@color/white" />
    <!-- 设置按钮的四个角为弧形 -->
    <!-- android:radius 弧形的半径 -->
    <corners android:radius="100dp" />
    <padding
        android:bottom="6dp"
        android:left="25dp"
        android:right="25dp"
        android:top="6dp" />

</shape>